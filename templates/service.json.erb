{
  "service": {
    "name": "<%= @name  %>",
    "tags": <%= @all_tags.to_json %>,
<% if @port %>
    "port": <%= @port %>,
<% end %>
    "check": {
      "name": "<%= @name %>",
<% if @check %>
      "script": "<%= @check %>",
<% end %>
<% if @tcp %>
      "tcp": "<%= @tcp %>",
<% end %>
<% if @http %>
      "http": "<%= @http %>",
<% end %>
      "interval": "<%= @interval %>"
    }
  }
}
